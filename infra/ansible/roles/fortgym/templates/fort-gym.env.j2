FORT_GYM_SETTINGS={{ fortgym_env_path }}
FORT_GYM_ADMIN_USER={{ lookup('env','FORT_GYM_ADMIN_USER') | default('admin', true) }}
FORT_GYM_ADMIN_PASSWORD={{ lookup('env','FORT_GYM_ADMIN_PASSWORD') | default('', true) }}
FORT_GYM_TRUST_PROXY=1
FORT_GYM_RATE_LIMIT_ENABLED=1
FORT_GYM_RATE_LIMIT_ADMIN_RPM={{ fortgym_rate_limit_admin_rpm | default('120', true) }}
FORT_GYM_RATE_LIMIT_RUNS_RPM={{ fortgym_rate_limit_runs_rpm | default('60', true) }}
DFHACK_ENABLED={{ '1' if dfhack_service_enabled else '0' }}
DF_PROTO_ENABLED=1
DFHACK_HOST=127.0.0.1
DFHACK_PORT={{ dfhack_remote_port }}
FORT_GYM_SEED_SAVE={{ fortgym_seed_save | default('', true) }}
FORT_GYM_RUNTIME_SAVE={{ fortgym_runtime_save | default('current', true) }}
ARTIFACTS_DIR={{ fortgym_install_dir }}/fort_gym/artifacts
FORT_GYM_DB_PATH={{ fortgym_install_dir }}/fort_gym/artifacts/fort_gym.sqlite3
OPENAI_API_KEY={{ lookup('env','OPENAI_API_KEY') | default('', true) }}
ANTHROPIC_API_KEY={{ lookup('env','ANTHROPIC_API_KEY') | default('', true) }}
